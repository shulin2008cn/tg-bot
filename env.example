# Telegram Bot Token (从 @BotFather 获取)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# 允许的群组ID列表 (用逗号分隔，可选)
ALLOWED_GROUP_IDS=-1001234567890,-1001234567891

# 服务器配置
PORT=3000
NODE_ENV=development

# 商品搜索API配置 (可选)
PRODUCT_API_KEY=your_api_key_here
PRODUCT_API_URL=https://api.example.com

# 代理配置 (如果在中国大陆需要代理访问Telegram)
PROXY_ENABLED=false
PROXY_HOST=localhost
PROXY_PORT=7890 